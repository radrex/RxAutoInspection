{
  "version": "1.0",
  "defaultProvider": "jsdelivr",
  "libraries": [
    {
      "library": "reset-css@5.0.1",
      "destination": "wwwroot/lib/reset-css/",
      "files": [
        "reset.css",
        "reset.min.css"
      ]
    },
    {
      "library": "bootstrap@4.4.1",
      "destination": "wwwroot/lib/bootstrap/",
      "files": [
        "dist/css/bootstrap.css",
        "dist/css/bootstrap.min.css",
        "dist/js/bootstrap.js",
        "dist/js/bootstrap.min.js",
        "dist/js/bootstrap.bundle.js",
        "dist/js/bootstrap.bundle.min.js"
      ]
    },
    {
      "library": "bootstrap-select@1.13.12",
      "destination": "wwwroot/lib/bootstrap/",
      "files": [
        "dist/css/bootstrap-select.css",
        "dist/css/bootstrap-select.min.css",
        "dist/js/bootstrap-select.js",
        "dist/js/bootstrap-select.min.js"
      ]
    },
    {
      "library": "@popperjs/core@2.1.1",
      "destination": "wwwroot/lib/popper/",
      "files": [
        "dist/umd/popper.js",
        "dist/umd/popper.min.js"
      ]
    },
    {
      "library": "jquery@3.4.1",
      "destination": "wwwroot/lib/jquery/",
      "files": [
        "dist/jquery.js",
        "dist/jquery.min.js"
      ]
    },
    {
      "library": "jquery-slim@3.0.0",
      "destination": "wwwroot/lib/jquery-slim/",
      "files": [
        "dist/jquery.slim.js",
        "dist/jquery.slim.min.js"
      ]
    },
    {
      "library": "jquery-validation@1.19.1",
      "destination": "wwwroot/lib/jquery-validation/",
      "files": [
        "dist/jquery.validate.js",
        "dist/jquery.validate.min.js"
      ]
    },
    {
      "library": "jquery-validation-unobtrusive@3.2.11",
      "destination": "wwwroot/lib/jquery-validation-unobtrusive/",
      "files": [
        "dist/jquery.validate.unobtrusive.js",
        "dist/jquery.validate.unobtrusive.min.js"
      ]
    }
  ]
}
