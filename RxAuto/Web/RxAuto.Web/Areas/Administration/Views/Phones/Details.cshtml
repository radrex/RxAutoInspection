@model RxAuto.Web.ViewModels.Phones.ViewModels.PhoneDetailsViewModel

@{
    ViewData["Title"] = "Details";
}

<h1>Details</h1>

<h4>Phone information</h4>

<div>
    <hr />
    <dl class="row">
        <dt class="col-sm-2">
            <label asp-for="@Model.PhoneNumber"></label>
        </dt>
        <dd class="col-sm-10">
            <label asp-for="@Model.PhoneNumber">@Model.PhoneNumber</label>
        </dd>
        <dt class="col-sm-2">
            <label asp-for="@Model.IsInternal"></label>
        </dt>
        <dd class="col-sm-10">
            <label asp-for="@Model.IsInternal">@Model.IsInternal</label>
        </dd>
    </dl>
</div>
<div>
    <a class="btn btn-info" asp-controller="Phones" asp-action="All">Back to List</a>
    <a class="btn btn-success" asp-controller="Phones" asp-action="Edit" asp-route-id="@Model.Id">Edit</a>
</div>